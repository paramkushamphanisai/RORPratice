<h1>Articals Listing page</h1>

<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Title</th>
            <th scope="col">Description</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
        <tbody>
        <% for i in @artical %>
            <tr>
            <td scope="col"><%= i.title %></td>
            <td scope="col"><%= i.description %></td>
            <td scope="col">Action</td>
            </tr>
        </tbody>
        <% end %>
    
</table>
